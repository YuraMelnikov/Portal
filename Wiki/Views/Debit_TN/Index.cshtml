            @Html.DisplayFor(modelItem => item.dateTN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.numberSF)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dateSF)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Summa)
        </td>
        <td>
