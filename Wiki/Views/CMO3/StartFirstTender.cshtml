@model Wiki.CMO_Order
@{
    Layout = null;
}
<head>
    <link href="~/Content/jquery-ui-1.12.1.custom/jquery-ui.css" rel="stylesheet" />
    <link href="~/Scripts/select2-3.5.4/select2.css" rel="stylesheet" />
    <script src="~/Content/jquery-ui-1.12.1.custom/jquery-ui.js"></script>
    <script src="~/Content/jquery-ui-1.12.1.custom/external/jquery/jquery.js"></script>
    <script src="~/Scripts/select2-3.5.4/select2.js"></script>
    <script src="~/Scripts/jquery-3.1.1.min.js"></script>
    <script src="~/Scripts/moment.js"></script>
    <script src="~/Scripts/moment-with-locales.js"></script>
    <script src="~/Scripts/moment-with-locales.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/bootstrap-datetimepicker.min.js"></script>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-datetimepicker.min.css" rel="stylesheet" />
</head>
<h2>Оформить заказ</h2>
@using (Html.BeginForm(Html.BeginForm("StartFirstTender", "CMO3", FormMethod.Post, new { enctype = "multipart/form-data" })))
{
    <h4>Выберете заявку/и: </h4>
    @Html.DropDownList("preOrder", ViewBag.preOrder as MultiSelectList, new { @class = "form-control" })
    <br />
    <h4>Укажите подрядчика: </h4>
    @Html.DropDownList("companyWin", ViewBag.CMO_Company as MultiSelectList, new { @class = "form-control" })
    <br />
    <h4>Укажите дату получения ответа </h4>
    @Html.EditorFor(model => model.dateCreate, new { htmlAttributes = new { @class = "form-control" } })
    <br />
    <h4>Цена, б/НДС (BYN): </h4>
    @Html.EditorFor(model => model.idTime, new { htmlAttributes = new { @class = "form-control" } })
    <hr />
    <input type="submit" value="Сохранить" class="btn btn-primary" />
    @Html.ActionLink("Назад", "ViewStartMenuOS", "CMO3", new { @class = "btn btn-default" })
}
<hr />
<script src="@Url.Content("~/Content/Chosen/chosen.jquery.js")" type="text/javascript"></script>
<link href="@Url.Content("~/Content/Chosen/chosen.css")" rel="stylesheet" type="text/css" />
<script>
    $(".chosen-select").chosen({
        placeholder_text_multiple: "Нажмите для выбора...",
        allow_single_deselect: true, disable_search_threshold: 5, width: "100%"
    });
</script>
<script type="text/javascript">
    $(function () {
        $('#dateCreate').datetimepicker({
            locale: 'ru',
            //format: 'L',
            calendarWeeks: true,
            showTodayButton: true
        });
    });
</script>