@model IEnumerable<Wiki.VV_Position>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.numMesto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Gabarit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.massaBrutto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.massaNetto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VVPZ.name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.numMesto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Gabarit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.massaBrutto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.massaNetto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VVPZ.name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
