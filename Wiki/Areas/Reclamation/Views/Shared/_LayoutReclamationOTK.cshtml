                <li><a href="" id="linkOrder" onclick="return deactiveList();">Замечания (список)</a></li>
                <li>
                    <a style="color:crimson" href="/Account/Login/">
                        @{Html.RenderAction("RenderUserMenu", "Remarks"); }
                    </a>
                </li>
            </ul>
        </div>
    </nav>
    <div class="container-fluid">
        @RenderBody()
    </div>
